<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <item identifier="mudraptorshell" descriptionidentifier="mudraptorshellmod" >
      <inherit identifier="mudraptorshell">
        <doinherit />
        <del sel="./InventoryIcon" />
        <replace sel="./Sprite/@texture" >
          %ModDir%/Content/Items/CreatureLoot/mudraptorshell_Item.png
        </replace>
        <replace sel="./Sprite/@sourcerect" >
          0,0,160,128
        </replace>
        <del sel="./Wearable/sprite[@name='shellshield']" />
        <add sel="./Wearable">
          <sprite name="mudraptorshell Helmet Wearable" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell_Item.png" limb="Head" inheritlimbdepth="true" inheritscale="true" ignorelimbscale="true" scale="0.65" hidelimb="false" sourcerect="4,144,100,100" origin="0.65,0.75"/>
          <sprite name="mudraptorshell Torso" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="Torso" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Hand" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightHand" hidelimb="true" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Hand" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftHand" hidelimb="true" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Lower Arm" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Lower Arm" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Upper Arm" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightForearm" hidelimb="false" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Upper Arm" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftForearm" hidelimb="false" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Waist" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="Waist" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Thigh" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightThigh" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Thigh" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftThigh" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Leg" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightLeg" hidelimb="false" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Leg" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftLeg" hidelimb="false" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Left Shoe" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="LeftFoot" sound="footstep_armor_heavy" hidelimb="true" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="mudraptorshell Right Shoe" texture="%ModDir%/Content/Items/CreatureLoot/mudraptorshell.png" limb="RightFoot" sound="footstep_armor_heavy" hidelimb="true" inherittexturescale="true" hideotherwearables="true" inheritorigin="true" inheritsourcerect="true" />
        </add>
        <!--boost a little bit-->
        <replace sel="./Wearable/StatusEffect[@type='OnWearing' and @target='Character']/@SpeedMultiplier" >
          0.7
        </replace>
      </inherit>
    </item>
  </Items>
</Override>