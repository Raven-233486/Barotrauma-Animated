<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <!-- UNIFORMS START-->
    <Item identifier="securityuniform1">
      <inherit identifier="securityuniform1">
        <doinherit />
        <del sel="./Wearable"/>
        <add sel=".">
          <Wearable slots="Any,InnerClothes" msg="ItemMsgPickUpSelect">
            <sprite name="Security Officer's Uniform 1 Torso" texture="SecurityOfficer1.png" limb="Torso" hidelimb="true" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="160,0,148,220" origin="0.5203,0.4364" />
            <sprite name="Security Officer's Uniform 1 Right Hand" texture="SecurityOfficer1.png" limb="RightHand" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="308,184,64,48" />
            <sprite name="Security Officer's Uniform 1 Left Hand" texture="SecurityOfficer1.png" limb="LeftHand" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="372,184,64,48" />
            <sprite name="Security Officer's Uniform 1 Left Lower Arm" texture="SecurityOfficer1.png" limb="LeftForearm" hidelimb="false" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="372,96,64,88" origin="0.5,0.3636" />
            <sprite name="Security Officer's Uniform 1 Right Lower Arm" texture="SecurityOfficer1.png" limb="RightForearm" hidelimb="false" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="308,96,64,88" origin="0.5,0.3636" />
            <sprite name="Security Officer's Uniform 1 Left Upper Arm" texture="SecurityOfficer1.png" limb="LeftArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="372,0,64,96" />
            <sprite name="Security Officer's Uniform 1 Right Upper Arm" texture="SecurityOfficer1.png" limb="RightArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="308,0,64,96" />
            <sprite name="Security Officer's Uniform 1 Waist" texture="SecurityOfficer1.png" limb="Waist" hidelimb="false" inheritorigin="true" inherittexturescale="true" inheritsourcerect="false" sourcerect="160,220,112,60" origin="0.5,0.53333" />
            <sprite name="Security Officer's Uniform 1 Right Thigh" texture="SecurityOfficer1.png" limb="RightThigh" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
            <sprite name="Security Officer's Uniform 1 Left Thigh" texture="SecurityOfficer1.png" limb="LeftThigh" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
            <sprite name="Security Officer's Uniform 1 Right Leg" texture="SecurityOfficer1.png" limb="RightLeg" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
            <sprite name="Security Officer's Uniform 1 Left Leg" texture="SecurityOfficer1.png" limb="LeftLeg" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
            <sprite name="Security Officer's Uniform 1 Right Shoe" texture="SecurityOfficer1.png" limb="RightFoot" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="308,232,64,48" />
            <sprite name="Security Officer's Uniform 1 Left Shoe" texture="SecurityOfficer1.png" limb="LeftFoot" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="372,232,64,48" />
            <damagemodifier afflictionidentifiers="blunttrauma,lacerations,gunshotwound,bitewounds" armorsector="0.0,360.0" damagemultiplier="0.8" />
          </Wearable>
        </add>
      </inherit>
    </Item>
    <Item identifier="securityuniform2">
      <inherit identifier="securityuniform2">
        <doinherit />
        <replace sel="./Wearable/sprite/@texture">
          SecurityOfficer2.png
        </replace>
        <replace sel="./Wearable/sprite/@hidelimb">
          false
        </replace>
        <replace sel="./Wearable/sprite[@limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='RightHand' or @limb='LeftHand']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
    <Item identifier="securityuniform3">
      <inherit identifier="securityuniform3">
        <doinherit />
        <replace sel="./Wearable/sprite/@texture">
          SecurityOfficer3.png
        </replace>
        <replace sel="./Wearable/sprite/@hidelimb">
          false
        </replace>
        <replace sel="./Wearable/sprite[@limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='RightHand' or @limb='LeftHand']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
    <!-- UNIFORMS END -->
    <!--修改贴图-->
    <Item identifier="bodyarmor">
      <inherit identifier="bodyarmor">
        <doinherit />
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/Mechanic/safetyharness.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Torso']/@texture">
          %ModDir%/Content/Items/Jobgear/Mechanic/safetyharness.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Torso']/@sourcerect">
          13,16,103,180
        </replace>
        <replace sel="./Wearable/sprite[@limb='Torso']/@origin">
          0.5,0.661
        </replace>
      </inherit>
    </Item>
    <!-- HEADGEAR START -->
    <Item identifier="ballistichelmet1">
      <inherit identifier="ballistichelmet1">
        <doinherit />
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          4,213,85,90
        </replace>
        <replace sel="./Sprite/@origin">
          0.5165,0.5
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@sourcerect">
          4,213,92,88
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@origin">
          0.5165,0.6001
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@scale">
          0.7
        </replace>
        <!-- Show hair -->
        <del sel="./Wearable/sprite[@limb='Head']/@hidewearablesoftype" />
        <add sel="./Wearable">
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="false" overlaycolor="0,0,0,0" range="3000" thermalgoggles="false" showdeadcharacters="false" showtexts="false" />
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="true" overlaycolor="176,0,0,120" range="3000" thermalgoggles="true" showdeadcharacters="false" showtexts="false">
            <RequiredItem items="thermalgoggles" type="Contained" />
          </StatusEffect>
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="true" overlaycolor="0,176,0,120" range="3000" thermalgoggles="false" showdeadcharacters="false" showtexts="true">
            <RequiredItem items="healthscanner" type="Contained" />
          </StatusEffect>
        </add>
        <add sel=".">
          <ItemContainer hideitems="true" capacity="1" maxstacksize="1" containedstateindicatorstyle="" containedspritedepths="0.551,0.552" showdeadcharacters="false" showtexts="false">
            <Containable items="Wf_helmet_module" />
            <SlotIcon slotindex="0" texture="%ModDir%/Content/items/weapons/Waifu_UI.png" sourcerect="120,0,120,120" origin="0.5,0.5" />
          </ItemContainer>
          <StatusHUD drawhudwhenequipped="false" overlaycolor="0,0,0,0" range="0" thermalgoggles="false" showdeadcharacters="false" showtexts="false" />
        </add>
      </inherit>
    </Item>
    <Item identifier="ballistichelmet2">
      <inherit identifier="ballistichelmet2">
        <doinherit />
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          110,207,104,107
        </replace>
        <replace sel="./Sprite/@origin">
          0.5105,0.4
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@sourcerect">
          110,207,104,107
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@origin">
          0.5105,0.523
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@scale">
          0.7
        </replace>
        <!-- Show hair -->
        <del sel="./Wearable/sprite[@limb='Head']/@hidewearablesoftype" />
        <add sel="./Wearable">
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="false" overlaycolor="0,0,0,0" range="3000" thermalgoggles="false" showdeadcharacters="false" showtexts="false" />
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="true" overlaycolor="176,0,0,120" range="3000" thermalgoggles="true" showdeadcharacters="false" showtexts="false">
            <RequiredItem items="thermalgoggles" type="Contained" />
          </StatusEffect>
          <StatusEffect type="OnWearing" target="This" targetitemcomponent="StatusHUD" drawhudwhenequipped="true" overlaycolor="0,176,0,120" range="3000" thermalgoggles="false" showdeadcharacters="false" showtexts="true">
            <RequiredItem items="healthscanner" type="Contained" />
          </StatusEffect>
        </add>
        <add sel=".">
          <ItemContainer hideitems="true" capacity="1" maxstacksize="1" containedstateindicatorstyle="" containedspritedepths="0.551,0.552">
            <Containable items="Wf_helmet_module" />
            <SlotIcon slotindex="0" texture="%ModDir%/Content/items/weapons/Waifu_UI.png" sourcerect="120,0,120,120" origin="0.5,0.5" />
          </ItemContainer>
          <StatusHUD drawhudwhenequipped="false" overlaycolor="0,0,0,0" range="0" thermalgoggles="false" showdeadcharacters="false" showtexts="false" />
        </add>
      </inherit>
    </Item>
    <!-- HEADGEAR END -->
  </Items>
</Override>