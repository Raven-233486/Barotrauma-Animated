    <item identifier="medicalofficerclothes">
      <inherit identifier="medicalofficerclothes">
        <doinherit />
        <!-- decrease spawnprobability -->
        <replace sel="./PreferredContainer/@spawnprobability">0.05</replace>
        <!-- increase Price and can't be sold -->
        <replace sel="./Price/@baseprice">150</replace>
        <add sel="./Price/@sold">false</add>
        <replace sel="./Wearable/sprite/@texture">MedicalOfficer.png</replace>
        <replace sel="./Wearable/sprite/@hidelimb">false</replace>
        <!-- add defence -->
        <add sel="./Wearable">
          <damagemodifier armorsector="0.0,360.0" afflictionidentifiers="blunttrauma,lacerations,bitewounds" damagemultiplier="0.9" />
        </add>
        <!-- add ItemContainer -->
        <add sel=".">
          <ItemContainer slotsperrow="1" capacity="4">
            <Containable items="chem,medical" excludeditems="toolbox,toolbelt" />
          </ItemContainer>
        </add>
